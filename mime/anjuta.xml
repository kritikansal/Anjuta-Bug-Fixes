<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-anjuta">
    <comment>Anjuta IDE Project</comment>
	<magic priority="80">
	   <match value="# Anjuta" type="string" offset="0"/>
       <match value="&lt;anjuta&gt;" type="string" offset="20:100"/>
     </magic>
    <glob pattern="*.anjuta"/>
    <glob pattern="*.prj"/>
  </mime-type>
  <mime-type type="application/x-anjuta-project-template">
    <sub-class-of type="application/xml"/>
    <comment>Anjuta IDE Project Template</comment>
    <glob pattern="*.wiz"/>
  </mime-type>
  <mime-type type="application/x-anjuta-compressed-project-template">
    <sub-class-of type="application/x-compressed-tar"/>
    <comment>Anjuta IDE Compressed Project Template</comment>
    <glob pattern="*.wiz.tgz"/>
  </mime-type>
</mime-info>
