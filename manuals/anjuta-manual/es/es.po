#, no-wrap
msgid ""
"\n"
"/* -*- Mode: C; indent-tabs-mode: t; c-basic-offset: 4; tab-width: 4 -*- */\n"
"  "
msgstr ""
"\n"
"/* -*- Mode: C; indent-tabs-mode: t; c-basic-offset: 4; tab-width: 4 -*- */\n"
"  "

#: C/language-support-cpp-modelines-emacs.page:31(page/p)
