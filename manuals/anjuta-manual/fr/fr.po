msgid "<code>ts</code>"
msgstr "<code>ts</code>"

#: C/language-support-cpp-modelines-vim.page:51(item/title)
msgid "<code>shiftwidth</code>"
msgstr "<code>shiftwidth</code>"

#: C/language-support-cpp-modelines-vim.page:52(item/title)
